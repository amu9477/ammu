pipeline {
  agent any 
     stages {
     stage ("install dependencies") {
         steps {
            sh 'npm install'
                }
            }
        stage ("test"){
           steps {
             sh 'echo "testing application"'
                 }
               }
         stage ('deploy bapplication'){
            steps {
              sh 'echo "deploying application"'
                   }
                   }
             }
             }
